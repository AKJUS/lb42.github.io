<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="css/pixi.css" media="screen" title="no title" charset="utf-8">
      <script src="js/CETEI.js" charset="utf-8"></script>
    <script type="text/javascript">
    var ceteicean = new CETEI(); 
    ceteicean.shadowCSS = "css/pixi.css";
    ceteicean.getHTML5('source/doc.xml', function(data) {
      document.getElementsByTagName("body")[0].appendChild(data);
    });
    </script>
    </head>
  <body>

  </body>
</html>
